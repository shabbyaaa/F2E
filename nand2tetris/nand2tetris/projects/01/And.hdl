/**
 * And gate: 
 * out = 1 if (a == 1 and b == 1)
 *       0 otherwise
 * 与门
 */

CHIP And {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a = a, b = b, out = w1);
    Nand(a = a, b = b, out = w2);
    Nand(a = w1, b = w2, out = out);
}
