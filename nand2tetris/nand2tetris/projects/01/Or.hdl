 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 * 或门
 * 思路：Or = Nand(Not(a), Not(b))
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(in = a, out = res1);
    Not(in = b, out = res2);
    Nand(a = res1, b = res2, out = out);
}
